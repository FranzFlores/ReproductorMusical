<div class="container">
    <h3 class="text-center">Listado de Canciones</h3>
    <table class="table table-hover">
        <tbody id="tableBody">
            <% songs.forEach(function(song){ %>
            <tr data-external="<%=song.external_id%>">
                <th scope="col"><%=song.number%></th>
                <td><%=song.title%></td>
                <td><%=song.listeners%></td>
            </tr>
            <% }); %>
        </tbody>
    </table>
</div>
